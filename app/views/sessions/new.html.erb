
  <!-- <%= notice %><%= alert %> -->
  <h1>ログイン</h1>
  <%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
    <div class="form-group">
      <%= f.label :Eメール %>
      <br>
      <%= f.email_field :email, class: 'form-control' %>
      <br>
      <%= f.label :パスワード %>
      <br>
      <%= f.password_field :password, class: 'form-control' %>
    </div>
  <br>
  <br>
    <%= f.submit "ログイン", class: 'btn btn-primary' %>
  <% end %>
